  <step>
    <name>civicrm_case_activity</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>cc5tudio_bccrs</connection>
    <sql>SELECT ca.case_id, clie.sort_name as client_name, cc.sort_name as reporter, a.activity_date_time as due_date,
                          a.status_id as status,
                          ov.name,
                          a.subject as subject,
                          a.is_deleted as deleted,
                          cl.modified_date as set_date,
                          IF( a.status_id=2, acc.sort_name, IF( a.status_id=3, acc.sort_name, NULL)) as completed_by ,
                          IF( a.status_id=2, cl.modified_date, IF( a.status_id=3, cl.modified_date, NULL)) as completed_date,
                          IF(a.original_id is NULL, cl.modified_date, al.modified_date) as create_date,stat.comments_177 as stat_check_comments,mct.comments_169 as mct_comments, task.comments_171 as other_task_comments, callback.comments_191 as callback_comments, assess.comments_183 as asse_comments,call_referral.comments_181 as call_referral_comments,offer.comments_193 as offerservices_comments, relative.comments_195 as relativedetails_comments, referral.comments_197 as referral_comments,provider.comments_199 as provider_comments,pac.comments_201 as pac_comments
 FROM civicrm_case_activity ca 
inner join civicrm_activity a on a.id=ca.activity_id
left join civicrm_value_stat_check__how_are_you_details_48 stat on stat.entity_id=a.id
left join civicrm_value_dispatch_mct_44 mct on mct.entity_id=a.id
left join civicrm_value_other_task_45 task on task.entity_id=a.id
left join civicrm_value_stat_check__call_referrals_detai_50 call_referral on call_referral.entity_id=a.id
left join civicrm_value_stat_check__assess_lethality_51 assess on assess.entity_id=a.id
left join civicrm_value_stat_check__if_no_call_back_deta_55 callback on callback.entity_id=a.id
left join civicrm_value_stat_check__offer_services_detai_56 offer on offer.entity_id=a.id
left join civicrm_value_stat_check__relative_details_57 relative on relative.entity_id=a.id
